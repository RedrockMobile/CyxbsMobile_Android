<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/store_iv_toolbar_arrow_left"
        android:layout_width="36dp"
        android:layout_height="18dp"
        android:layout_marginStart="3dp"
        android:background="?attr/selectableItemBackgroundBorderless"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075"
        app:srcCompat="@drawable/store_ic_arrow_left"
        tools:ignore="ContentDescription" />

    <View
        android:id="@+id/store_common_toolbar_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="11dp"
        android:background="@drawable/store_shape_divide_line"
        app:layout_constraintTop_toBottomOf="@+id/store_iv_toolbar_arrow_left" />

</androidx.constraintlayout.widget.ConstraintLayout>