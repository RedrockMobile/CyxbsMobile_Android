<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="arrangementMode" format="enum">
        <enum name="fill" value="0" />
        <enum name="normal" value="1" />
    </attr>
    <declare-styleable name="NineGridView">
        <attr name="arrangementMode" />
    </declare-styleable>

    <declare-styleable name="LikeView">
        <attr format="color" name="defaultColor"></attr>
        <attr format="color" name="checkedColor"></attr>
        <attr format="color" name="ringColor"></attr>
        <attr format="dimension" name="cirRadius"></attr>
        <attr format="integer" name="cycleTime"></attr>
        <attr format="integer" name="unSelectCycleTime"></attr>
        <attr format="reference" name="defaultLikeIconRes"></attr>
        <attr format="reference" name="checkedLikeIconRes"></attr>
        <attr format="float" name="lrGroupCRatio"></attr>
        <attr format="float" name="lrGroupBRatio"></attr>
        <attr format="float" name="bGroupACRatio"></attr>
        <attr format="float" name="tGroupBRatio"></attr>
        <attr format="integer" name="innerShapeScale"></attr>
        <attr format="integer" name="dotSizeScale"></attr>
        <attr format="boolean" name="allowRandomDotColor"></attr>
    </declare-styleable>
</resources>